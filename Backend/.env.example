# JWT Configuration
JWT_SECRET=your_secure_256bit_secret_key_here
JWT_EXPIRATION=86400000

# Google OAuth2
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
OAUTH2_REDIRECT_URI=http://localhost:4200/oauth2/redirect

# Frontend URL
APP_FRONTEND_URL=http://localhost:4200

# MongoDB
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net
MONGODB_DATABASE=volunteering_prod

# Servergit checkout -b PDBLC-18-social-authentication
SERVER_PORT=8080

# Email Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_app_specific_password
MAIL_PROPERTIES_MAIL_SMTP_AUTH=true
MAIL_PROPERTIES_MAIL_SMTP_STARTTLS_ENABLE=true
MAIL_FROM=noreply@yourapp.com
MAIL_PERSONAL=Volunteering Platform 

# File Storage
FILE_UPLOAD_DIR=./uploads
# CORS Configuration
ALLOWED_ORIGINS=http://localhost:4200 